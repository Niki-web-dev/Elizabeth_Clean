
<section class="hero">
  <div class="hero__bg">{% render 'wave-icon' %}</div> 

  <div class="container relative">
    <div class="hero__img">
      <img src="{{ 'hero.png' | asset_img_url: '920x675' }}" alt="nuddle" loading="lazy" height="675" width="920">
    </div>
    <div class="hero__content">
      <div class="hero__wrapper">
        <h1 class="hero__title">Nutritionally <span class="color-f6">complete</span><br>instant noodles</h1>
        <p class="hero__subtitle color-f2">Plant-based instant noodles that are healthy for you and our planet</p>
        <a href="/" class="button">Noodles store</a>
      </div>
    </div>
  </div>

  <div class="rounded__text">
    <div class="rotate-block">{% render 'rounded-text' %}</div>
    <div class="arrow">{% render 'arrow' %}</div>
