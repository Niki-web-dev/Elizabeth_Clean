</section>

<section class="noodle">
  <div class="noodle__bg">{% render 'noodle-line' %}</div>

  <div class="container relative">
    <img src="{{ 'parsley.png' | asset_img_url: '450x450' }}" alt="parsley" loading="lazy" class="parsley" width="100" height="100">
