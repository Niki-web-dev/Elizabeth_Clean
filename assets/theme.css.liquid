@font-face {
  font-family: "Benzin-Semibold";
  src: local("Benzin Semibold"), local("Benzin-Semibold"),
    url('{{ "Benzin-Semibold.woff2" | asset_url }}') format("woff2"),
    url('{{ "Benzin-Semibold.woff" | asset_url }}') format("woff"),
    url('{{ "Benzin-Semibold.ttf" | asset_url }}') format("truetype");
  font-weight: 600;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "MAXWELLBOLD";
  src: local("MAXWELLBOLD"),
    url('{{ "maxwell-bold.woff2" | asset_url }}') format("woff2"),
    url('{{ "MAXWELLBOLD.ttf" | asset_url }}') format("ttf");
  font-weight: 600;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: "PT Sans Narrow";
  src: local("Pt Sans Narrow"), local("Pt-Sans-Narrow"),
    url('{{ "PTSansNarrow-Regular.ttf" | asset_url }}') format("ttf");
  font-weight: 600;
  font-style: normal;
  font-display: swap;
}

* {
  box-sizing: border-box;
}

:root {
  --blue-TUF: #0034c6;
  --white: #fff;
  --f2-white: #f2dfde;
  --f6-pink: #f6d6d6;
  --fs-96: 96px;
  --fs-90: 90px;
  --fs-88: 88px;
  --fs-80: 80px;
  --fs-70: 70px;
  --fs-60: 60px;
  --fs-54: 54px;
  --fs-52: 52px;
  --fs-48: 48px;
  --fs-44: 44px;
  --fs-40: 40px;
  --fs-36: 36px;
  --fs-32: 32px;
  --fs-28: 28px;
  --fs-24: 24px;
  --fs-20: 20px;
  --red: #e23c13;
  --yellow: #f09e00;
  --black: #272728;
  --black-2c: #2c2c2c;
  --black-18: #181818;
}

.container {
  margin: 0 auto;
  height: 100%;
  width: auto;
  padding: 0px 100px;
  max-width: 1440px;
}

.color-f6 {
  color: var(--f6-pink);
}

.color-blue {
  color: var(--blue-TUF);
}

h1 {
  color: var(--white);
  font-family: "MAXWELLBOLD";
  font-size: var(--fs-90);
  font-style: normal;
  font-weight: 400;
  line-height: 98%; /* 88.713px */
  letter-spacing: 1.358px;
  text-transform: uppercase;
}

h2 {
  margin: 0;
  font-size: 100px;
  line-height: 108%; /* 108px */
  text-align: center;
  font-family: "MAXWELLBOLD";
}

p {
  margin: 0;
}

.relative {
  position: relative;
}

body {
  font-family: "PT Sans Narrow", -apple-system, BlinkMacSystemFont, "Segoe UI",
    "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans",
    "Helvetica Neue", sans-serif;
  color: #fff;
  background-color: var(--f6-pink);
  margin: 0;
}

a,
.logo,
.logo-2 {
  font-family: inherit;
  color: inherit;
  text-decoration: none;
}

/* start nav */

.overflow-hidden {
  overflow: hidden;
}

.burger__line {
  display: block;
  width: 28px;
  height: 1px;
  background-color: var(--white);
  transition: all 0.5s ease;
  transform-origin: center center;
}

.nav__burger {
  display: none;
  cursor: pointer;
}

.checkout__btn {
  position: absolute;
  right: -50px;
  margin-top: 5px;
  top: 50%;
}

.mobile-logo {
  display: none;
}

.nav {
  padding: 30px 0 50px;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 100;
  transition: background-color 0.5s ease;
}

.nav.active {
  background-color: var(--blue-TUF);
}

.nav__link {
  color: var(--white);
  font-family: "MAXWELLBOLD";
  font-size: var(--fs-20);
  font-style: normal;
  font-weight: 400;
  line-height: 130%; /* 18.2px */
  letter-spacing: 0.42px;
  transition: color 0.3s ease;
}

.nav__link.mobile {
  display: none;
}

.nav__link:hover {
  color: var(--red);
}

.nav__links {
  display: flex;
  gap: 20px;
}

.nav__links-bg {
  display: none;
}

.nav__links .logo-2 {
  display: none;
}

.nav__content {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.nav__logo-wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  left: 0;
  right: 0;
  overflow: hidden;
  z-index: -1;
  bottom: -30px;
}
.logo {
  width: 105px;
}
.logo-divider {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  top: 9px;
  height: 2.5px;
  margin-left: -91px;
  width: 100%;
  background-color: var(--white);
}
.logo-block {
  background-color: var(--white);
  display: block;
  width: 9px;
  height: 20px;
}

.logo-block-1 {
  margin-top: 1px;
}

.logo-2 {
  width: 50px;
  align-self: end;
}
/* end nav */

.hero__bg {
  position: absolute;
  width: 100%;
  object-fit: cover;
  z-index: 0;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.hero {
  height: 770px;
  width: 100%;
  border-radius: 0px 0px 70px 70px;
  background: var(--blue-TUF);
  position: relative;
  z-index: 10;
}

.hero__subtitle {
  color: var(--f2-white);
  font-family: PT Sans Narrow;
  font-size: var(--fs-28);
  font-style: normal;
  font-weight: 400;
  line-height: 130%; /* 36.4px */
  max-width: 426px;
}

.hero__content {
  margin: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 100%;
  max-width: 750px;
  z-index: 1;
  position: relative;
}
.hero__title {
  margin-bottom: 12px;
}

.hero__subtitle {
  margin-bottom: 40px;
}

.hero__img {
  overflow: hidden;
  position: absolute;
  bottom: 0;
  right: 0;
  width: 900px;
}

.hero__img img {
  display: block;
  width: 100%;
  height: 100%;
  transform: translateY(100%);
  transition: transform 0.7s ease;
}

.hero__img.active img {
  transform: translateY(0%);
}

.button {
  color: var(--white);
  font-family: "MAXWELLBOLD";
  font-size: 32px;
  font-style: normal;
  font-weight: 400;
  line-height: 130%; /* 41.6px */
  letter-spacing: 1.28px;
  display: inline-flex;
  padding: 14px 70px 16px 70px;
  justify-content: center;
  align-items: center;
  border-radius: 64px;
  background: var(--red);
  transition: all 0.3s ease;
}

.button:hover {
  background-color: var(--white);
  color: var(--red);
}

.arrow {
  height: 42px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 2;
  width: 42px;
}

.rounded__text {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 50%);
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120px;
  height: 120px;
  background-color: #e23c13;
  border-radius: 50%;
}

@keyframes rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(-360deg);
  }
}

.rotate-block {
  animation: rotate 5s linear infinite;
  width: 112px;
  height: 108px;
}

.noodle {
  padding-top: 200px;
  padding-bottom: 162px;
  position: relative;
  margin-top: -80px;
  z-index: 9;
  background-color: var(--f6-pink);
}
.parsley {
  width: 176px;
  height: 177px;
  position: absolute;
  z-index: -1;
  top: -10px;
  left: 10px;
  transition: transform 0.4s linear;
}

.noodle__bg {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  width: 100%;
  object-fit: fill;
  object-position: 50% 50%;
  margin-top: 76px;
  z-index: -2;
  height: 856px;
}

.noodle__icon {
  height: 72px;
  display: flex;
  margin-bottom: 25px;
  align-items: flex-end;
  justify-content: center;
}

.mushrooms {
  position: absolute;
  width: 137px;
  height: 115px;
  z-index: -1;
  top: 360px;
  right: 0;
  transition: transform 0.7s linear;
}
.noodle__title {
  margin-bottom: 52px;
  color: var(--red);
}
.noodle__advantage-wrapper {
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 75px;
  max-width: 1114px;
}
.noodle__content {
  display: flex;
  flex-direction: column;
  text-align: center;
  max-width: 247px;
  justify-content: center;
}
.noodle__advantage {
  color: var(--black);
  text-align: center;
  font-family: "MAXWELLBOLD";
  font-size: 44px;
  line-height: 130%; /* 57.2px */
}
.noodle__descr {
  color: var(--black);
  font-size: 28px;
  line-height: 130%; /* 36.4px */
}

.noodle__button {
  display: flex;
  justify-content: center;
}

@keyframes move-logo {
  0% {
    transform: translateX(100%);
  }
  100% {
    transform: translateX(-100%);
  }
}

.loop {
  width: 100%;
  display: flex;
  overflow: hidden;
}

.loop__content.active {
  animation: move-logo 9.9s linear;
}

.loop__content {
  width: 100%;
  transform: translateX(100%);
}

.big-logo {
  min-width: 100%;
  width: 100%;
  object-fit: cover;
}

.product {
  padding: 115px 0;
}

.product__grid {
  display: grid;
  grid-template-columns: repeat(3, 282px);
  row-gap: 16px;
  grid-auto-columns: 282px;
  justify-content: space-between;
}
.product__box {
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.product__img {
  position: relative;
  margin-bottom: 24px;
  display: flex;
  justify-content: center;
}
.product__main-img {
  position: relative;
  z-index: 2;
  transition: opacity 0.3s ease;
}
.product__second-img {
  opacity: 0;
  z-index: 1;
}

.product__second-img,
.product__bg-circle {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  transition: opacity 0.3s ease;
}
.product__bg-circle {
  margin-top: -20px;
  width: 322px;
  opacity: 0.4;
  height: 319px;
  color: var(--white);
  transition: all 0.3s ease;
  z-index: 0;
}
.product__name {
  color: #63257c;
  text-align: center;
  font-family: "MAXWELLBOLD";
  font-size: 44px;
  font-weight: 400;
  line-height: 120%; /* 52.8px */
}

.product__box:nth-child(2) .product__name {
  color: var(--red);
}

.product__box:nth-child(3) .product__name {
  color: #f09e00;
}

.product__box:nth-child(2) .product__btn {
  background-color: var(--red);
}
.product__box:nth-child(3) .product__btn {
  background-color: #f09e00;
}

.product__description {
  text-align: center;
  font-size: 18px;
  font-weight: 400;
  line-height: 132.5%; /* 23.85px */
  color: var(--black-2c);
  margin-bottom: 14px;
}
.product__btn {
  opacity: 0;
  pointer-events: none;
  padding: 15px 70px;
  margin: 0 auto;
  color: var(--white);
  font-size: 32px;
  font-family: "MAXWELLBOLD";
  font-weight: 400;
  line-height: 130%; /* 41.6px */
  letter-spacing: 1.28px;
  border-radius: 64px;
  background: #63257c;
  transition: opacity, color, background-color 0.3s ease;
}

.product__btn:hover {
  background-color: var(--white);
  color: var(--black-2c);
}

.product__box:hover .product__btn {
  opacity: 1;
  pointer-events: initial;
}

.product__box:hover .product__main-img {
  opacity: 0;
}

.product__box:hover .product__second-img {
  opacity: 1;
}
.product__box:hover .product__bg-circle {
  color: #0a7334;
  opacity: 1;
}

