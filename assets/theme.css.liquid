/*.slick-active .insta__info.top {
transform: translate(-50%, -150%);
}
.slick-active .insta__info.bottom {
transform: translate(-50%, 150%);
}*/
.slick-slide .insta__info.top,
.slick-slide[aria-hidden="true"]:not(.slick-cloned)
  ~ .slick-cloned[aria-hidden="true"]
  .insta__info.top {
  transform: translate(-50%, 0%);
  transition: all 1000ms ease;
}
.slick-center .insta__info.top,
.slick-slide[aria-hidden="true"]:not([tabindex="-1"])
  + .slick-cloned[aria-hidden="true"]
  .insta__info.top {
  transform: translate(-50%, -150%);
}
.slick-slide .insta__info.bottom,
.slick-slide[aria-hidden="true"]:not(.slick-cloned)
  ~ .slick-cloned[aria-hidden="true"]
  .insta__info.bottom {
  transform: translate(-50%, 0%);
  transition: all 1000ms ease;
}
.slick-center .insta__info.bottom,
.slick-slide[aria-hidden="true"]:not([tabindex="-1"])
  + .slick-cloned[aria-hidden="true"]
  .insta__info.bottom {
  transform: translate(-50%, 150%);
}
.slick-slide .insta__main-img,
.slick-slide[aria-hidden="true"]:not(.slick-cloned)
  ~ .slick-cloned[aria-hidden="true"]
  .insta__main-img {
  transform: scale(1.2, 1.2);
  transition: all 1000ms ease;
}
.slick-center .insta__main-img,
.slick-slide[aria-hidden="true"]:not([tabindex="-1"])
  + .slick-cloned[aria-hidden="true"]
  .insta__main-img {
  /* max-width: 200%;
  max-height: 200%;
  width: 200%;
  height: 200%; */
  transform: scale(1.4, 1.4);
}

