.insta__top {
  grid-column-gap: 12px;
  grid-row-gap: 12px;
  flex-wrap: wrap;
  align-items: center;
  display: flex;
}

.insta__logo {
  width: 50px;
  height: 50px;
  background-color: #e23c13;
  border-radius: 50%;
  justify-content: center;
  align-items: center;
  display: flex;
}

.insta__logo-img {
  object-fit: cover;
  width: 39px;
  height: 16px;
}

.insta__title {
  color: #2d2936;
  font-size: 16px;
  font-family: sans-serif;
  font-weight: 700;
}

.insta__counter-wrapper {
  grid-column-gap: 12px;
  grid-row-gap: 12px;
  flex-direction: column;
  align-items: flex-start;
  display: flex;
}

.insta__buttons {
  grid-column-gap: 12px;
  grid-row-gap: 12px;
  display: flex;
}

.insta__social {
  width: 30px;
  height: 30px;
}

.insta__icon {
  width: 100%;
  height: 100%;
}

.insta .container {
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.footer {
  flex-direction: row;
  justify-content: space-between;
  display: flex;
  overflow: hidden;
}

.footer__img {
  width: 100%;
  max-height: 828px;
  object-fit: cover;
}

.footer__content {
  width: 100%;
  background-color: #e23c13;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 70px 50px;
  display: flex;
}

.footer__logo-img {
  max-width: 491px;
}

.footer__logo {
  margin-bottom: 52px;
}

.footer__links {
  width: 100%;
  justify-content: space-around;
  margin-top: 22px;
  display: flex;
}

.footer__container {
  max-width: 491px;
}

.footer__link {
  color: #fff;
  font-size: 16px;
  line-height: 130%;
  text-decoration: none;
  transition: color 0.3s;
}

.footer__link:hover {
  color: #0034c6;
}

.bg-blue {
  background-color: #0034c6;
  border-radius: 64px;
}

.map {
  margin-top: 40px;
}

.map__img {
  width: 100%;
  object-fit: cover;
}

.map__preloader {
  position: absolute;
  z-index: 2;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  transition: all 1s ease;
}

.map__preloader img {
  width: 100%;
  object-fit: cover;
}

.map__title {
  max-width: 974px;
  text-align: center;
  margin: 0 auto 45px auto;
  display: block;
}

.insta__wrapper {
  position: relative;
}

.map__wrapper,
.map__preloader img {
  height: 630px;
  max-height: 90vh;
}

.footer .button {
  background-color: var(--blue-TUF);
  text-align: center;
  position: relative;
  z-index: 3;
}

.footer .button:hover {
  background-color: var(--f2-white);
}

.footer__logo {
  margin-bottom: 54px;
  position: relative;
  z-index: 3;
}

.footer__container {
  display: flex;
  flex-direction: column;
}

.footer {
  overflow: hidden;
}

.footer__bg {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 0;
  object-fit: cover;
}

.footer__content {
  position: relative;
}

.footer__links {
  position: relative;
  z-index: 3;
}

/* start MEDIA queries */

@media screen and (max-width: 1300px) {
  .advatages__main-bg {
    width: 36vw;
  }

  .plate {
    width: 39vw;
  }

  .slick-center .insta__main-img,
  .slick-slide[aria-hidden="true"]:not([tabindex="-1"])
    + .slick-cloned[aria-hidden="true"]
    .insta__main-img {
    transform: scale(1.5, 1.5);
  }
}

@media screen and (max-width: 1200px) {
  .hero__img {
    width: 55vw;
    right: 20px;
  }

  h1 {
    font-size: var(--fs-70);
  }

  .product__grid {
    grid-template-columns: 1fr;
  }

  .container {
    padding: 0 40px;
  }

  .insta__slide {
    margin: 0px 8vw;
  }

  .advantages__conten {
    gap: 40px;
